var _interopRequireWildcard = require("@babel/runtime/helpers/interopRequireWildcard");

var actions = _interopRequireWildcard(require("../actions/index"));

describe('actions', function () {
  it('should have a type of SET_PROFILE_DATA', function () {
    var profileData = {
      name: 'Jev',
      upcomingRides: 3
    };
    var expectedAction = {
      type: 'SET_PROFILE_DATA',
      profileData: profileData
    };
    var result = actions.setProfileData(profileData);
    expect(result).toEqual(expectedAction);
  });
  it('should have a type of SET_RIDE_DATA', function () {
    var rideData = {};
    var expectedAction = {
      type: 'SET_RIDE_DATA',
      rideData: rideData
    };
    var result = actions.setRideData(rideData);
    expect(result).toEqual(expectedAction);
  });
  it('should have a type of TOGGLE_LOGIN', function () {
    var bool = !bool;
    var expectedAction = {
      type: 'TOGGLE_LOGIN',
      bool: bool
    };
    var result = actions.toggleLogin(bool);
    expect(result).toEqual(expectedAction);
  });
  it('should have a type of TOGGLE_CURRENT_FRIEND', function () {
    var num = 3;
    var expectedAction = {
      type: 'TOGGLE_CURRENT_FRIEND',
      num: num
    };
    var result = actions.toggleCurrentFriend(num);
    expect(result).toEqual(expectedAction);
  });
  it('should have a type of TOGGLE_CURRENT_RIDE', function () {
    var num = 4;
    var expectedAction = {
      type: 'TOGGLE_CURRENT_RIDE',
      num: num
    };
    var result = actions.toggleCurrentRide(num);
    expect(result).toEqual(expectedAction);
  });
});
